<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <ApplicationIcon>PenForSign.ico</ApplicationIcon>
    <RootNamespace>ASNR</RootNamespace>
    <AssemblyName>ASNR</AssemblyName>
    <Description>Remove/Resign assemblies' StrongName automatically.</Description>
    <Copyright>Copyright Â© Ulysses 2016-2022</Copyright>
    <Authors>wdwxy12345@gmail.com</Authors>
    <PackageId>AutoStrongNameRemover</PackageId>
    <Product>AutoStrongNameRemover</Product>
    <PackageProjectUrl>https://github.com/UlyssesWu/ASNR</PackageProjectUrl>
    <RepositoryUrl>https://github.com/UlyssesWu/ASNR</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="PenForSign.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="dnlib" Version="3.5.0" />
  </ItemGroup>

</Project>
